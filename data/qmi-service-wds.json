
[
  {  "name"    : "QMI Client WDS",
     "type"    : "Client" },

  {  "name"    : "QMI Message WDS",
     "type"    : "Message-ID-Enum" },

  {  "name"    : "Start Network",
     "type"    : "Message",
     "service" : "WDS",
     "id"      : "0x0020",
     "input"   : [  { "name"      : "APN",
                      "id"        : "0x14",
                      "mandatory" : "no",
                      "type"      : "TLV",
                      "format"    : "string" },
                    { "name"      : "Username",
                      "id"        : "0x17",
                      "mandatory" : "no",
                      "type"      : "TLV",
                      "format"    : "string" },
                    { "name"      : "Password",
                      "id"        : "0x18",
                      "mandatory" : "no",
                      "type"      : "TLV",
                      "format"    : "string" } ],
     "output"  : [  { "name"      : "Result",
                      "type"      : "common-TLV" },
                    { "name"      : "Packet Data Handle",
                      "id"        : "0x01",
                      "mandatory" : "yes",
                      "type"      : "TLV",
                      "format"    : "guint32",
                      "prerequisite": { "field"     : "Result.Error Status",
                                        "operation" : "==",
                                        "value"     : "QMI_STATUS_SUCCESS" } },
                    { "name"      : "Call End Reason",
                      "id"        : "0x10",
                      "mandatory" : "no",
                      "type"      : "TLV",
                      "format"    : "guint16",
                      "prerequisite": [ { "field"     : "Result.Error Status",
                                          "operation" : "!=",
                                          "value"     : "QMI_STATUS_SUCCESS" },
                                        { "field"     : "Result.Error Code",
                                          "operation" : "==",
                                          "value"     : "QMI_PROTOCOL_ERROR_CALL_FAILED" } ] },
                    { "name"      : "Verbose Call End Reason",
                      "id"        : "0x11",
                      "mandatory" : "no",
                      "type"      : "TLV",
                      "format"    : "struct",
                      "contents"  : [ { "name" : "Type",
                                        "type" : "guint16" },
                                      { "name" : "Reason",
                                        "type" : "guint16" } ],
                      "prerequisite": [ { "field"     : "Result.Error Status",
                                          "operation" : "!=",
                                          "value"     : "QMI_STATUS_SUCCESS" },
                                        { "field"     : "Result.Error Code",
                                          "operation" : "==",
                                          "value"     : "QMI_PROTOCOL_ERROR_CALL_FAILED" } ] } ] },

  {  "name"    : "Stop Network",
     "type"    : "Message",
     "service" : "WDS",
     "id"      : "0x0021",
     "input"   : [  { "name"      : "Packet Data Handle",
                      "id"        : "0x01",
                      "mandatory" : "yes",
                      "type"      : "TLV",
                      "format"    : "guint32" } ],
     "output"  : [  { "name"      : "Result",
                      "type"      : "common-TLV" } ] },

  {  "name"    : "Get Packet Service Status",
     "type"    : "Message",
     "service" : "WDS",
     "id"      : "0x0022",
     "output"  : [  { "name"      : "Result",
                      "type"      : "common-TLV" },
                    { "name"      : "Connection Status",
                      "id"        : "0x01",
                      "mandatory" : "yes",
                      "type"      : "TLV",
                      "format"    : "guint8",
                      "prerequisite": { "field"     : "Result.Error Status",
                                        "operation" : "==",
                                        "value"     : "QMI_STATUS_SUCCESS" } } ] }

]
